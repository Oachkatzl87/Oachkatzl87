void fetchTheStars(){
    while(!onBeeper()){
        fetchBalloon(); 
    }
}
void fetchBalloon(){
    turnLeft();
    repeat(5){
        moveForward();
    }
    if(!frontIsClear()){
        turnAround();
        pickBeeper();
        repeat(5){
            moveForward();
        }    
    }
    else{
        moveForward();
        if(!frontIsClear()){
            turnAround();
            pickBeeper();
            repeat(6){
                moveForward();
            }    
        }
        else{
            moveForward();
            turnAround();
            pickBeeper();
            repeat(7){
                moveForward();
            }
        }
    }
    turnLeft();
    dropBeeper();
    if(frontIsClear()){
        moveForward();
    }
}
