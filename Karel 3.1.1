void partyAgain(){
    while(onBeeper()){
        hangBalloon(); 
    }
}
void hangBalloon(){
    pickBeeper();
    turnLeft();
    repeat(5){
        moveForward();
    }
    if(!frontIsClear()){
        turnAround();
        dropBeeper();
        repeat(5){
            moveForward();
        }    
    }
    else{
        moveForward();
        if(!frontIsClear()){
            turnAround();
            dropBeeper();
            repeat(6){
                moveForward();
            }    
        }
        else{
            moveForward();
            turnAround();
            dropBeeper();
            repeat(7){
                moveForward();
            }
        }
    }
    turnLeft();
    if(beeperAhead()){
        moveForward();
    }
}
